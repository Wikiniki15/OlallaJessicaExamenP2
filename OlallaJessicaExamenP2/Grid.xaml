<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="OlallaJessicaExamenP2.Grid"
             Title="Grid">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <BoxView BackgroundColor="Black" HeightRequest="2" Grid.RowSpan="2" Grid.ColumnSpan="2"/>
        <BoxView BackgroundColor="Black" WidthRequest="2" Grid.RowSpan="2" Grid.ColumnSpan="2"/>

        <Label Text="Jessica" Grid.Row="0" Grid.Column="0" HorizontalOptions="Center" VerticalOptions="Center" />
        <Label Text="Nicole" Grid.Row="0" Grid.Column="1" HorizontalOptions="Center" VerticalOptions="Center"/>
        <Label Text="Olalla" Grid.Row="1" Grid.Column="0" HorizontalOptions="Center" VerticalOptions="Center"/>
        <Label Text="Inca" Grid.Row="1" Grid.Column="1" HorizontalOptions="Center" VerticalOptions="Center"/>

    </Grid>
</ContentPage>