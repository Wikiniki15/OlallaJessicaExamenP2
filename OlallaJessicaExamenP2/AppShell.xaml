<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="OlallaJessicaExamenP2.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:OlallaJessicaExamenP2"
    Shell.FlyoutBehavior="Disabled"
    Title="OlallaJessicaExamenP2">

    <TabBar>
        <Tab Title="Recarga" Icon="recarga2.png">
            <ShellContent
            Title="Recarga"
            ContentTemplate="{DataTemplate local:Recarga}"
            Route="Recarga" />
        </Tab>

        <Tab Title="Grid" Icon="grid.png">
            <ShellContent
            Title="Grid"
            ContentTemplate="{DataTemplate local:Grid}"
            Route="Grid" />
        </Tab>

    </TabBar>

</Shell>
